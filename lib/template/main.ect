<!DOCTYPE html>
</html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no">

    <% if @twitter != "" || @facebook != "" : %>
      <meta property="og:url" content="<%= @origin %>">
      <meta property="og:type" content="website" />
      <meta property="og:title" content="<%= @title %>" />
      <meta property="og:description" content="<%= @description %>" />
      <meta property="og:site_name" content="<%= @site_name %>" />
      <meta property="og:image" content="<%= @thumbnail %>" />

      <% if @twitter != "" : %>
        <meta name="twitter:card" content="summary" />
        <meta name="twitter:site" content="@<%= @twitter %>" />
      <% end %>
      <% if @facebook != "" : %>
        <meta property="fb:app_id" content="<%= @facebook %>" />
      <% end %>
    <% end %>

    <title><%= @title %></title>

    <% for f in @cssfilelist : %>
      <link rel="stylesheet" type="text/css" href="<%= f %>">
    <% end %>
    <% for f in @jssyslist : %>
      <script type="text/javascript" src="<%= f %>"></script>
    <% end %>

    <script type="text/javascript">
      node_env = "<%= @node_env %>";
      pkgname = "<%= @pkgname %>";
    </script>

    <script type="text/javascript" src="<%= @pkgname %>/syslib/plustick.min.js"></script>
    <script type="text/javascript" src="<%= @pkgname %>/syslib/main.min.js"></script>
    <% for f in @jsuserlist : %>
      <script type="text/javascript" src="<%= f %>"></script>
    <% end %>
  </head>
  <body style="margin: 0px 0px 0px 0px; overflow:hidden; background-color:rgba(0, 0, 0, 0.9);"></body>
</html>

