<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="UTF-8" />
    <meta name="description" content="<%= @description %>">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=4,user-scalable=yes" />

    <% if @twitter != "" || @facebook != "" : %>
      <% if @ogpimg != "" : %>
        <meta property="og:image" content="<%= @origin %><%= @ogpimg %>" />
      <% end %>
      <meta property="og:url" content="<%= @site_url %>" />
      <meta property="og:type" content="website" />
      <meta property="og:title" content="<%= @title %>" />
      <meta property="og:description" content="<%= @description %>" />
      <meta property="og:site_name" content="<%= @site_name %>" />

      <% if @twitter != "" : %>
        <meta name="twitter:card" content="summary_large_image" />
        <meta name="twitter:site" content="@<%= @twitter %>" />
      <% end %>
      <% if @facebook != "" : %>
        <meta property="fb:app_id" content="<%= @facebook %>" />
      <% end %>
    <% end %>

    <title><%= @title %></title>
    <% if @favimg != "" : %>
      <link rel="icon" href="<%= @favimg %>" />
    <% end %>
    <link rel="apple-touch-icon" href="<%= @favimg %>">

    <% if @PWA == "activate" : %>
      <link rel="manifest" href="<%= @manifest %>" />
    <% end %>

    <link rel="stylesheet" type="text/css" href="<%= @systemcss %>" />
    <% for f in @cssfilelist : %>
      <link rel="stylesheet" type="text/css" href="<%= f %>" />
    <% end %>

    <script type="text/javascript">
      NODE_ENV = "<%= @NODE_ENV %>";
      pkgname = "<%= @pkgname %>";
      PWA = "<%= @PWA %>";
    </script>

    <!--
    <% if @PWA == "activate" : %>
      <script async type="text/javascript" src="https://cdn.jsdelivr.net/npm/pwacompat" crossorigin="anonymous"></script>
    <% end %>
    -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.21.1/axios.min.js" defer></script>
  <script type="text/javascript" src="<%= @pkgname %>/syslib/plustick.min.js" defer></script>
    <% for f in @jssyslist : %>
      <script async type="text/javascript" src="<%= f %>" defer></script>
    <% end %>
  </head>
  <body style="margin: 0px 0px 0px 0px; overflow:hidden; background-color:rgba(255, 255, 255, 1.0); overscroll-behavior:none;">
    <script type="text/javascript" src="<%= @pkgname %>/syslib/main.min.js" defer></script>
    <script type="text/javascript" src="<%= @pkgname %>/view/appsmain.min.js" defer></script>
  </body>
</html>

