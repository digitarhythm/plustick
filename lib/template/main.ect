<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no" />

    <% if @twitter != "" || @facebook != "" : %>
      <% if @ogpimg != "" : %>
        <meta property="og:image" content="<%= @origin %>/<%= @ogpimg %>" />
      <% end %>
      <meta property="og:url" content="<%= @origin %>" />
      <meta property="og:type" content="website" />
      <meta property="og:title" content="<%= @title %>" />
      <meta property="og:description" content="<%= @description %>" />
      <meta property="og:site_name" content="<%= @site_name %>" />

      <% if @twitter != "" : %>
        <meta name="twitter:card" content="summary_large_image" />
        <meta name="twitter:site" content="@<%= @twitter %>" />
      <% end %>
      <% if @facebook != "" : %>
        <meta property="fb:app_id" content="<%= @facebook %>" />
      <% end %>
    <% end %>

    <title><%= @title %></title>
    <% if @favimg != "" : %>
      <link rel="icon" href="<%= @origin %>/<%= @pkgname %>/public/<%= @favimg %>" />
    <% end %>
    <link rel="apple-touch-icon" href="/public/img/icon/icon-192x192.png">

    <% if @manifest != "" : %>
      <link rel="manifest" href="<%= @manifest %>" />
    <% end %>

    <link rel="stylesheet" type="text/css" href="<%= @systemcss %>" />
    <% for f in @cssfilelist : %>
      <link rel="stylesheet" type="text/css" href="<%= f %>" />
    <% end %>

    <script type="text/javascript">
      node_env = "<%= @node_env %>";
      pkgname = "<%= @pkgname %>";
    </script>

    <script async src="https://cdn.jsdelivr.net/npm/pwacompat" crossorigin="anonymous"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.21.1/axios.min.js" defer></script>
    <script type="text/javascript" src="<%= @pkgname %>/syslib/plustick.min.js" defer></script>
    <script type="text/javascript" src="<%= @pkgname %>/syslib/main.min.js" defer></script>
    <script type="text/javascript" src="<%= @pkgname %>/view/appsmain.min.js" defer></script>
    <% for f in @jssyslist : %>
      <script type="text/javascript" src="<%= f %>" defer></script>
    <% end %>
  </head>
  <body style="margin: 0px 0px 0px 0px; overflow:hidden; background-color:rgba(255, 255, 255, 1.0); overscroll-behavior:none;">
  </body>
</html>

